class Shape:
author= 'Ashwin Mehta'
def _init_(self,x,y):
self.x=x
self.y=y
def area(self,x,y):
self.x=x
self.y=y
a=self.x*self.y
print ('Area of a rectangle',a)
print (author)
class Square(Shape): 
def _init_(self,x):
self.x=x
def area(self,x):
self.x=x
a= self.x*self.x
print('Area of a square',a)
shape = Shape(5, 6)
shape.area(5, 6)  
square = Square(4)
square.area(4)  
